{
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "tsc",
    "builds": [
        {
            "src": "/build/index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/index.js"
        }
    ]
}